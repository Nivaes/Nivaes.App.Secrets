{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Playground+Benchmarks",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_benchmark",
      "program": "${workspaceFolder}/test/Nivaes.IoC.Benchmarks/bin/Release/net9.0/Nivaes.IoC.Benchmarks.dll",
      "args": [],
      "cwd": "${workspaceFolder}/test/Nivaes.IoC.Benchmarks",
      // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart",
      "requireExactSource": false // allows to for moderate code editing while debugging
    },
    {
      "name": "Playground+Benchmarks (Publish)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "publish_benchmark",
      "program": "${workspaceFolder}/test/Nivaes.IoC.Benchmarks/bin/Release/net9.0/Nivaes.IoC.Benchmarks.exe",
      "args": [],
      "cwd": "${workspaceFolder}/test/Nivaes.IoC.Benchmarks",
      // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart",
      "requireExactSource": false // allows to for moderate code editing while debugging
    },
  ]
}
